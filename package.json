{
  "name": "openfisca-web-ui",
  "version": "0.2.0",
  "description": "Web user interface for OpenFisca -- a versatile microsimulation free software",
  "keywords": [
    "benefit",
    "interface",
    "microsimulation",
    "social",
    "tax",
    "user",
    "server",
    "web"
  ],
  "main": "openfisca_web_ui/static/js/index.js",
  "scripts": {
    "build": "NODE_ENV=production browserify . > openfisca_web_ui/static/js/bundle.js",
    "start": "watchify -o openfisca_web_ui/static/js/bundle.js -v -d .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openfisca/openfisca-web-ui.git"
  },
  "author": "Christophe Benz <christophe.benz@data.gouv.fr>",
  "license": "AGPLv3+",
  "bugs": {
    "url": "https://github.com/openfisca/openfisca-web-ui/issues"
  },
  "dependencies": {
    "bootstrap": "^3.1.1",
    "jquery": "^2.1.1",
    "q": "^1.0.1",
    "underscore": "^1.6.0",
    "backbone": "^1.1.2",
    "X-editable": "git+https://github.com/vitalets/x-editable.git",
    "d3": "^3.4.8",
    "nvd3": "git+https://github.com/novus/nvd3.git",
    "envify": "^1.2.1",
    "react": "^0.10.0",
    "reactify": "^0.13.1",
    "map-object": "0.0.2",
    "superagent": "^0.18.0"
  },
  "private": true,
